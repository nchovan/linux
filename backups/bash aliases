alias clr=clear                                                                                     
alias doc='cl ~/Documents'                                                                      
alias grep='grep --color=auto'                                                                  
alias testfile='cd ~/Documents/testfiles'                                                       
alias home='cl ~'                                                                               
alias open_password='vim ~/Documents/password.txt'                                               
alias open_bills='vim ~/Documents/bills.txt'
alias open_vimrc='vim ~/.vimrc'                                    
alias open_aliases='vim ~/.bash_aliases'                                             
alias bills_payed='~/github_repos/linux_bash_scripts/./bills_payed.sh'
alias update_bashscript_repo='cp ~/linux_script/*.sh ~/github_repos/linux_bash_scripts/'
alias gpush='branch=$(git symbolic-ref --short HEAD); git push --set-upstream origin $branch'
alias tnote='~/tnote/tnote/./tnote'
alias linbash='cd ~/github_repos/linux_bash_scripts'
alias pbcopy='xclip -selection clipboard'
alias pbpaste='xclip -selection clipboard -o'
alias tmux='~/./tmux_test_config.sh'
